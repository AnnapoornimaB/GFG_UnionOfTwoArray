class Solution
{
    public static int doUnion(int arr1[], int arr2[])
    {
        int c = 0;
        Set<Integer> m = new HashSet<Integer>();
        for(int i : arr1)
        {
            m.add(i);
        }
        for(int j : arr2)
        {
            m.add(j);
        }
        for(int i : m)
        {
            c++;
        }
    return c;
    }
}
